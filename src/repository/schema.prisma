datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Tools {
  id    Int     @id @default(autoincrement())
  title String  
  link  String
  description String 
  tags Tags[]
}

model Tags {
  id  Int @id @default(autoincrement())
  name  String
  tools Tools[]
}